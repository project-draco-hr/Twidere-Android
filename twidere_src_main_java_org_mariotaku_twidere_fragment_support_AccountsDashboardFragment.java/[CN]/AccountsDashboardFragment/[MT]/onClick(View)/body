{
switch (v.getId()) {
case R.id.profile_container:
{
      final ParcelableAccount account=mAccountsAdapter.getSelectedAccount();
      if (account == null)       return;
      final FragmentActivity activity=getActivity();
      IntentUtils.openUserProfile(activity,account.account_key,account.account_key.getId(),account.screen_name,null,true,UserFragment.Referral.SELF_PROFILE);
      break;
    }
}
}
