{
switch (v.getId()) {
case R.id.profile_container:
{
      final ParcelableAccount account=mAccountsAdapter.getSelectedAccount();
      if (account == null)       return;
      final FragmentActivity activity=getActivity();
      IntentUtils.openUserProfile(activity,new AccountKey(account.account_id,account.account_host),account.account_id,account.screen_name,null,true,UserFragment.Referral.SELF_PROFILE);
      break;
    }
}
}
