{
  final SharedPreferences.Editor editor=getEditor();
  if (editor == null)   return;
  final Location item=mAdapter.getItem(which);
  if (item != null) {
    editor.putInt(KEY_LOCAL_TRENDS_WOEID,item.getWoeid());
    editor.apply();
  }
  if (mDialog != null && mDialog.isShowing()) {
    mDialog.dismiss();
  }
}
