{
  for (  final StreamListener listener : listeners) {
    final JSONObject deletionNotice=json.getJSONObject("delete");
    if (deletionNotice.has("status")) {
      ((StatusListener)listener).onDeletionNotice(new StatusDeletionNoticeImpl(deletionNotice.getJSONObject("status")));
    }
 else {
      final JSONObject directMessage=deletionNotice.getJSONObject("direct_message");
      ((UserStreamListener)listener).onDeletionNotice(InternalParseUtil.getLong("id",directMessage),InternalParseUtil.getLong("user_id",directMessage));
    }
  }
}
