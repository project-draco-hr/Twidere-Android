{
  final JSONObject deletionNotice=json.getJSONObject("delete");
  if (deletionNotice.has("status")) {
    for (    final StreamListener listener : listeners) {
      ((SiteStreamsListener)listener).onDeletionNotice(forUser.get(),new StatusDeletionNoticeImpl(deletionNotice.getJSONObject("status")));
    }
  }
 else {
    final JSONObject directMessage=deletionNotice.getJSONObject("direct_message");
    for (    final StreamListener listener : listeners) {
      ((SiteStreamsListener)listener).onDeletionNotice(forUser.get(),InternalParseUtil.getInt("id",directMessage),InternalParseUtil.getLong("user_id",directMessage));
    }
  }
}
