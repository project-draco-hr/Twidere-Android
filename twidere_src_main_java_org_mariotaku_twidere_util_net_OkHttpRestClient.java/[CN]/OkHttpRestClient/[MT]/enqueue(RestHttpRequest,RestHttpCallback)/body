{
  final Call call;
  try {
    call=newCall(request);
  }
 catch (  final MalformedURLException e) {
    final DummyRequest dummyCall=new DummyRequest();
    client.getDispatcher().getExecutorService().execute(new Runnable(){
      @Override public void run(){
        if (dummyCall.isCancelled()) {
          callback.cancelled();
          return;
        }
        callback.exception(e);
      }
    }
);
    return dummyCall;
  }
  call.enqueue(new Callback(){
    @Override public void onFailure(    final Request request,    final IOException e){
      if (call.isCanceled()) {
        callback.cancelled();
        return;
      }
      callback.exception(e);
    }
    @Override public void onResponse(    final Response response) throws IOException {
      if (call.isCanceled()) {
        callback.cancelled();
        return;
      }
      callback.callback(new OkRestHttpResponse(response));
    }
  }
);
  return new OkHttpQueuedRequest(client,call);
}
