{
  for (  final Object section : mSections.keySet()) {
    final Adapter adapter=mSections.get(section);
    final int size=adapter.getCount() + 1;
    if (position == 0)     return section;
    if (position < size)     return adapter.getItem(position - 1);
    position-=size;
  }
  return null;
}
