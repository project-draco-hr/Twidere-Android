{
  final String display_name=UserColorNameUtils.getDisplayName(getContext(),user_id,name,screen_name,name_first,nickname_only,false);
  reply_retweet_status.setText(count > 1 ? getString(R.string.retweeted_by_name_with_count,display_name,count - 1) : getString(R.string.retweeted_by_name,display_name));
  reply_retweet_status.setCompoundDrawablesWithIntrinsicBounds(R.drawable.ic_indicator_retweet,0,0,0);
}
