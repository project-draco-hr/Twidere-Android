{
  if (items == null || name == null)   return;
  try {
    final File file=JSONFileIO.getSerializationFile(getContext(),name);
    final List<UnreadItem> restored=JSONFileIO.readArrayList(file);
    if (restored != null) {
      items.addAll(restored);
    }
  }
 catch (  final IOException e) {
    e.printStackTrace();
  }
}
