{
  final AbsStatusesAdapter<Data> adapter=getAdapter();
  final ParcelableStatus status=adapter.getStatus(statusPosition);
  if (status == null)   return;
  IntentUtils.openMedia(getActivity(),status,media,null,true);
  final MediaEvent event=MediaEvent.create(getActivity(),status,media,getTimelineType(),adapter.isMediaPreviewEnabled());
  HotMobiLogger.getInstance(getActivity()).log(new AccountKey(status.account_id,status.account_host),event);
}
