{
  StringBuilder sb=new StringBuilder();
  boolean isEscaped=false;
  while (cur < len) {
    char ch=s.charAt(cur++);
    if (isEscaped) {
      sb.append(ch);
      isEscaped=false;
    }
 else     if (ch == ESCAPE) {
      isEscaped=true;
    }
 else     if (ch == DIVIDER_KEY_VALUE || ch == DIVIDER_OBJECT_FIELD || ch == DIVIDER_ARRAY_ELEMENT) {
      lastToken=ch;
      break;
    }
 else {
      sb.append(ch);
    }
  }
  return sb.toString();
}
