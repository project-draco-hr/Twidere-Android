{
  boolean deleted=false;
  try {
    for (int i=s.length() - 1; i >= 0; i--) {
      if (s.charAt(i) == '\n') {
        s.delete(i,i + 1);
        deleted|=true;
      }
    }
  }
 catch (  IndexOutOfBoundsException e) {
    throw new IndexOutOfBoundsException("Error processing " + s + ", original message: "+ e.getMessage());
  }
  return deleted;
}
