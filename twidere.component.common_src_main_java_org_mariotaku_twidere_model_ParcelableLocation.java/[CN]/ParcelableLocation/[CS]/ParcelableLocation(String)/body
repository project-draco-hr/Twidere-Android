{
  if (locationString == null) {
    latitude=-1;
    longitude=-1;
    return;
  }
  final String[] longlat=locationString.split(",");
  if (longlat.length != 2) {
    latitude=-1;
    longitude=-1;
  }
 else {
    latitude=ParseUtils.parseDouble(longlat[0]);
    longitude=ParseUtils.parseDouble(longlat[1]);
  }
}
