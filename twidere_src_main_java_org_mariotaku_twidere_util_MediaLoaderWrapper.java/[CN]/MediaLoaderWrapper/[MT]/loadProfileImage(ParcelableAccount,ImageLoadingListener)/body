{
  if (account.account_user != null && account.account_user.extras != null && !TextUtils.isEmpty(account.account_user.extras.profile_image_url_profile_size)) {
    loadProfileImage(account.account_user.extras.profile_image_url_profile_size,listener);
  }
 else {
    loadProfileImage(account.profile_image_url,listener);
  }
}
