{
  Date date=null;
  try {
    date=parseTwitterDate(string);
  }
 catch (  NumberFormatException e) {
    Log.w("Twidere",e);
  }
  if (date != null)   return date;
  try {
    date=mDateFormat.parse(string);
  }
 catch (  ParseException e) {
    BugReporter.error("Unrecognized date: " + string,e);
    return null;
  }
  return date;
}
