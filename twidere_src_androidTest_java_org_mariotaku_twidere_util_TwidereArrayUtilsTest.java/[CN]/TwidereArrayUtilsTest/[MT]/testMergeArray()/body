{
  String[] array1={"1","2"};
  String[] array2={"1","2"};
  String[] array3=null;
  String[] merged=new String[TwidereArrayUtils.arraysLength(array1,array2,array3)];
  TwidereArrayUtils.mergeArray(merged,array1,array2,array3);
  String[] expected={"1","2","1","2"};
  assertArrayEquals(expected,merged);
}
