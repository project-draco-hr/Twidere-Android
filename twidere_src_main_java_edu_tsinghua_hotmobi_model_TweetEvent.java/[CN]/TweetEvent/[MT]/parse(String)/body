{
  if (OPEN.value.equalsIgnoreCase(action)) {
    return OPEN;
  }
 else   if (RETWEET.value.equalsIgnoreCase(action)) {
    return RETWEET;
  }
 else   if (FAVORITE.value.equalsIgnoreCase(action)) {
    return FAVORITE;
  }
 else   if (UNFAVORITE.value.equalsIgnoreCase(action)) {
    return UNFAVORITE;
  }
  return UNKNOWN;
}
