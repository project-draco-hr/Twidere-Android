{
  final String line=String.format(Locale.US,"MESH:%d, TEX_OES:%d, TEX_RECT:%d, FILL_RECT:%d, LINE:%d",mCountDrawMesh,mCountTextureRect,mCountTextureOES,mCountFillRect,mCountDrawLine);
  mCountDrawMesh=0;
  mCountTextureRect=0;
  mCountTextureOES=0;
  mCountFillRect=0;
  mCountDrawLine=0;
  Log.d(TAG,line);
}
