{
switch (view.getId()) {
case R.id.close:
{
      onBackPressed();
      break;
    }
case R.id.send:
{
    if (isQuotingProtectedStatus()) {
      new RetweetProtectedStatusWarnFragment().show(getSupportFragmentManager(),"retweet_protected_status_warning_message");
    }
 else {
      updateStatus();
    }
    break;
  }
case R.id.select_account:
{
  Toast.makeText(this,"Select account",Toast.LENGTH_SHORT).show();
  break;
}
}
}
