{
switch (view.getId()) {
case R.id.error_container:
{
      getUserListInfo(true);
      break;
    }
case R.id.profile_image:
{
    final ParcelableUserList userList=mUserList;
    if (userList == null)     return;
    IntentUtils.openUserProfile(getActivity(),userList.account_id,userList.user_id,userList.user_screen_name,null,true,null);
    break;
  }
}
}
