{
  final Bundle args=getArguments();
  if (args != null) {
    final long account_id=args.getLong(EXTRA_ACCOUNT_ID,-1);
    final long user_id=args.getLong(EXTRA_USER_ID,-1);
    final String screen_name=args.getString(EXTRA_SCREEN_NAME);
    if (account_id == user_id || screen_name != null && screen_name.equals(getAccountScreenName(getActivity(),account_id)))     return true;
  }
  return false;
}
