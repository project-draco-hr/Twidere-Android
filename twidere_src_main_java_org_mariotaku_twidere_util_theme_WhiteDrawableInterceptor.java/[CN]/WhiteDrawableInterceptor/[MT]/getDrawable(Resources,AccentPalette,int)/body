{
  if (resId == R.drawable.solid_pressed_white_intercepted)   return new ColorDrawable(mAccentPalette.getAccentColor(PRESSED_ALPHA));
  if (resId == R.drawable.solid_focused_white_intercepted)   return new ColorDrawable(mAccentPalette.getAccentColor(FOCUSED_ALPHA));
  if (resId == R.drawable.rect_focused_background_white_intercepted) {
    final int backColor=mAccentPalette.getAccentColor(0x55);
    final int borderColor=mAccentPalette.getAccentColor(0xAA);
    return new RectDrawable(mResources,backColor,2f,borderColor);
  }
  return null;
}
