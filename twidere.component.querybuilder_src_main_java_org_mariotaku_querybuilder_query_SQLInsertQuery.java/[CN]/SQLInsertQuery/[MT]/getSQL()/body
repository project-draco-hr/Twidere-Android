{
  if (table == null)   throw new NullPointerException("table must not be null!");
  final StringBuilder sb=new StringBuilder();
  sb.append("INSERT ");
  if (onConflict != null) {
    sb.append("OR ");
    sb.append(onConflict.getAction());
    sb.append(" ");
  }
  sb.append("INTO ");
  sb.append(table);
  sb.append(" (");
  sb.append(Utils.toString(columns,',',true));
  sb.append(") ");
  sb.append(values);
  return sb.toString();
}
