{
  try {
switch (authType) {
case AuthType.OAUTH:
      return authOAuth();
case AuthType.XAUTH:
    return authxAuth();
case AuthType.BASIC:
  return authBasic();
case AuthType.TWIP_O_MODE:
return authTwipOMode();
}
return authOAuth();
}
 catch (final MicroBlogException e) {
Log.w(LOGTAG,e);
return new SignInResponse(false,false,e);
}
catch (final AuthenticationException e) {
Log.w(LOGTAG,e);
return new SignInResponse(false,false,e);
}
}
