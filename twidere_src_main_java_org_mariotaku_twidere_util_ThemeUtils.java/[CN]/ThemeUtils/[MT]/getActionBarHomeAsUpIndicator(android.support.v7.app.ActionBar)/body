{
  final Context context=actionBar.getThemedContext();
  @SuppressWarnings("ConstantConditions") final TypedArray a=context.obtainStyledAttributes(null,R.styleable.ActionBar,R.attr.actionBarStyle,0);
  try {
    return a.getDrawable(R.styleable.ActionBar_homeAsUpIndicator);
  }
  finally {
    a.recycle();
  }
}
