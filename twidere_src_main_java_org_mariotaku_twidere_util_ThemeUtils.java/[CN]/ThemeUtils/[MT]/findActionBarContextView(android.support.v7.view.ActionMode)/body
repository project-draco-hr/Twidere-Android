{
  if (modeCompat instanceof ActionModeImpl) {
    WindowDecorActionBar actionBar=(WindowDecorActionBar)Utils.findFieldOfTypes(modeCompat,ActionModeImpl.class,WindowDecorActionBar.class);
    if (actionBar == null)     return null;
    return (View)Utils.findFieldOfTypes(actionBar,WindowDecorActionBar.class,ActionBarContextView.class);
  }
 else   if (modeCompat instanceof StandaloneActionMode) {
    return (View)Utils.findFieldOfTypes(modeCompat,StandaloneActionMode.class,ActionBarContextView.class);
  }
  return null;
}
