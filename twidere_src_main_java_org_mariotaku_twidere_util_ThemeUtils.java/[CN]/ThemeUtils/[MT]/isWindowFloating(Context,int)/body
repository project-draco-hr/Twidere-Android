{
  final TypedArray a;
  if (theme != 0) {
    a=context.obtainStyledAttributes(null,new int[]{android.R.attr.windowIsFloating},0,theme);
  }
 else {
    a=context.obtainStyledAttributes(new int[]{android.R.attr.windowIsFloating});
  }
  try {
    return a.getBoolean(0,false);
  }
  finally {
    a.recycle();
  }
}
