{
  if (VALUE_THEME_NAME_TWIDERE.equals(name) || VALUE_THEME_NAME_LIGHT.equals(name))   return darkActionBar ? R.style.Theme_Twidere_Settings_Light_DarkActionBar : R.style.Theme_Twidere_Settings_Light;
 else   if (VALUE_THEME_NAME_DARK.equals(name))   return R.style.Theme_Twidere_Settings_Dark;
  return R.style.Theme_Twidere_Settings_Light_DarkActionBar;
}
