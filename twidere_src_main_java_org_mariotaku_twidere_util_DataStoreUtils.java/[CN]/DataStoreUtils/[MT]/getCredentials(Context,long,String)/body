{
  Cursor cur=context.getContentResolver().query(Accounts.CONTENT_URI,Accounts.COLUMNS,Expression.equals(Accounts.ACCOUNT_ID,accountId).getSQL(),null,null);
  if (cur == null)   return null;
  try {
    ParcelableCredentialsCursorIndices i=new ParcelableCredentialsCursorIndices(cur);
    cur.moveToFirst();
    while (!cur.isAfterLast()) {
      if (TextUtils.equals(cur.getString(i.account_host),accountHost)) {
        return i.newObject(cur);
      }
      cur.moveToNext();
    }
    if (cur.moveToFirst()) {
      return i.newObject(cur);
    }
  }
  finally {
    cur.close();
  }
  return null;
}
