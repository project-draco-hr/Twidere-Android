{
  final String[] projection={Accounts.ACCOUNT_TYPE};
  String[] whereArgs={String.valueOf(accountKey.getId()),accountKey.getHost()};
  Cursor cur=context.getContentResolver().query(Accounts.CONTENT_URI,projection,Utils.getAccountCompareExpression().getSQL(),whereArgs,null);
  if (cur == null)   return null;
  try {
    if (cur.moveToFirst()) {
      return cur.getString(0);
    }
  }
  finally {
    cur.close();
  }
  return null;
}
