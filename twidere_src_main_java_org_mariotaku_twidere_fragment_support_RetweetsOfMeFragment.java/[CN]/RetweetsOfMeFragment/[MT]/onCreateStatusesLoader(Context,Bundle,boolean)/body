{
  final UserKey accountKey=args.getParcelable(EXTRA_ACCOUNT_KEY);
  final String maxId=args.getString(EXTRA_MAX_ID);
  final String sinceId=args.getString(EXTRA_SINCE_ID);
  final int tabPosition=args.getInt(EXTRA_TAB_POSITION,-1);
  return new RetweetsOfMeLoader(context,accountKey,sinceId,maxId,getAdapterData(),getSavedStatusesFileArgs(),tabPosition,fromUser);
}
