{
switch (item.getItemId()) {
case MENU_DELETE:
{
      final long[] itemIds=mListView.getCheckedItemIds();
      final Expression where=Expression.in(new Column(Tabs._ID),new RawItemArray(itemIds));
      mResolver.delete(Tabs.CONTENT_URI,where.getSQL(),null);
      break;
    }
}
mode.finish();
return true;
}
