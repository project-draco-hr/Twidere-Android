{
  this.mime=mime;
  this.charset=charset;
  this.filename=filename;
  this.contentDisposition=contentDisposition;
  this.data=data;
  if (null == mime) {
    throw new NullPointerException("`mime' must not be null");
  }
  if (null == data) {
    throw new NullPointerException("`data' must not be null");
  }
}
