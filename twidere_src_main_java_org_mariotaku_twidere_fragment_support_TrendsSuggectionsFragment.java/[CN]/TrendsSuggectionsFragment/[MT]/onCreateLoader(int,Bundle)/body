{
  final Uri uri=CachedTrends.Local.CONTENT_URI;
  final String table=getTableNameByUri(uri);
  final String where=table != null ? CachedTrends.TIMESTAMP + " = " + "(SELECT "+ CachedTrends.TIMESTAMP+ " FROM "+ table+ " ORDER BY "+ CachedTrends.TIMESTAMP+ " DESC LIMIT 1)" : null;
  return new CursorLoader(getActivity(),uri,CachedTrends.COLUMNS,where,null,null);
}
