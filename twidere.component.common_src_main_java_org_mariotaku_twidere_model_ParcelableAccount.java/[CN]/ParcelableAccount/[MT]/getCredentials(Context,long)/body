{
  if (context == null)   return null;
  Cursor cur=ContentResolverUtils.query(context.getContentResolver(),Accounts.CONTENT_URI,Accounts.COLUMNS,Accounts.ACCOUNT_ID + " = " + accountId,null,null);
  if (cur == null)   return null;
  try {
    if (cur.moveToFirst()) {
      return ParcelableCredentialsCursorIndices.fromCursor(cur);
    }
  }
  finally {
    cur.close();
  }
  return null;
}
