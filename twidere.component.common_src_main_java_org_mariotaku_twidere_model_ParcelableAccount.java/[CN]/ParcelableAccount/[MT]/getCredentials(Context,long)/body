{
  if (context == null)   return null;
  try (Cursor cur=ContentResolverUtils.query(context.getContentResolver(),Accounts.CONTENT_URI,Accounts.COLUMNS,Accounts.ACCOUNT_ID + " = " + accountId,null,null)){
    if (cur != null && cur.moveToFirst()) {
      return ParcelableCredentialsCursorIndices.fromCursor(cur);
    }
    return null;
  }
 }
