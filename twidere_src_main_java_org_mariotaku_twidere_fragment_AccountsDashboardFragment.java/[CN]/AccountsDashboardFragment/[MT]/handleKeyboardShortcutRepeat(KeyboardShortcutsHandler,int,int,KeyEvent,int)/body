{
  final String action=handler.getKeyAction(CONTEXT_TAG_NAVIGATION,keyCode,event,metaState);
  if (action == null)   return false;
  final int offset;
switch (action) {
case ACTION_NAVIGATION_PREVIOUS:
{
      offset=-1;
      break;
    }
case ACTION_NAVIGATION_NEXT:
{
    offset=1;
    break;
  }
default :
{
  return false;
}
}
return true;
}
