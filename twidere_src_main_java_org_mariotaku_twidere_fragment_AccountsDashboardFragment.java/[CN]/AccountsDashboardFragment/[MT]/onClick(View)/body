{
switch (v.getId()) {
case R.id.profileContainer:
{
      final ParcelableAccount account=mAccountsAdapter.getSelectedAccount();
      if (account == null)       return;
      final FragmentActivity activity=getActivity();
      if (account.account_user != null) {
        IntentUtils.openUserProfile(activity,account.account_user,null,preferences.getBoolean(KEY_NEW_DOCUMENT_API),Referral.SELF_PROFILE);
      }
 else {
        IntentUtils.openUserProfile(activity,account.account_key,account.account_key,account.screen_name,null,preferences.getBoolean(KEY_NEW_DOCUMENT_API),Referral.SELF_PROFILE);
      }
      break;
    }
}
}
