{
  if (throwable == null && message == null) {
    throw new NullPointerException("Message and Throwable can't be both null");
  }
  if (message != null) {
    if (BuildConfig.DEBUG) {
      Log.w(LOGTAG,message,throwable);
    }
    return;
  }
  if (BuildConfig.DEBUG) {
    Log.w(LOGTAG,throwable);
  }
}
