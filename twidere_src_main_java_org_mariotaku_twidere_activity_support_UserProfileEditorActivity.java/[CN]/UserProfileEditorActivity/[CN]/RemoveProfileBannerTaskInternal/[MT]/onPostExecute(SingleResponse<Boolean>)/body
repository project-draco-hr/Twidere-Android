{
  super.onPostExecute(result);
  if (result.getData() != null && result.getData()) {
    getUserInfo();
    Toast.makeText(UserProfileEditorActivity.this,R.string.profile_banner_image_updated,Toast.LENGTH_SHORT).show();
  }
 else {
    showErrorMessage(UserProfileEditorActivity.this,R.string.action_removing_profile_banner_image,result.getException(),true);
  }
  setUpdateState(false);
}
