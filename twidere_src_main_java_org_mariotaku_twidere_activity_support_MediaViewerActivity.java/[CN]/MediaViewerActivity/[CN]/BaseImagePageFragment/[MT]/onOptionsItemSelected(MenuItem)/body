{
switch (item.getItemId()) {
case R.id.open_in_browser:
{
      openInBrowser();
      return true;
    }
case R.id.save:
{
    requestAndSaveToGallery();
    return true;
  }
case R.id.refresh:
{
  loadMedia();
  return true;
}
case R.id.share:
{
shareMedia();
return true;
}
}
return super.onOptionsItemSelected(item);
}
