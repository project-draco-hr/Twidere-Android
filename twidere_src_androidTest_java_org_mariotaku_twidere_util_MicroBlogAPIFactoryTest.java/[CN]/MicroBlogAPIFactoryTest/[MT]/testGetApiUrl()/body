{
  assertEquals("https://api.twitter.com/",MicroBlogAPIFactory.getApiUrl("https://[DOMAIN.]twitter.com","api",null));
  assertEquals("https://api.twitter.com/",MicroBlogAPIFactory.getApiUrl("https://[DOMAIN.]twitter.com/","api",null));
  assertEquals("https://api.twitter.com/1.1/",MicroBlogAPIFactory.getApiUrl("https://[DOMAIN.]twitter.com","api","/1.1/"));
  assertEquals("https://api.twitter.com/1.1",MicroBlogAPIFactory.getApiUrl("https://[DOMAIN.]twitter.com/","api","1.1"));
  assertEquals("https://api.twitter.com/1.1",MicroBlogAPIFactory.getApiUrl("https://[DOMAIN.]twitter.com","api","/1.1"));
  assertEquals("https://api.twitter.com/1.1",MicroBlogAPIFactory.getApiUrl("https://[DOMAIN.]twitter.com/","api","/1.1"));
  assertEquals("https://api.twitter.com/1.1/",MicroBlogAPIFactory.getApiUrl("https://[DOMAIN.]twitter.com","api","1.1/"));
  assertEquals("https://api.twitter.com/1.1/",MicroBlogAPIFactory.getApiUrl("https://[DOMAIN.]twitter.com/","api","1.1/"));
  assertEquals("https://api.twitter.com/1.1/",MicroBlogAPIFactory.getApiUrl("https://[DOMAIN.]twitter.com","api","/1.1/"));
  assertEquals("https://api.twitter.com/1.1/",MicroBlogAPIFactory.getApiUrl("https://[DOMAIN.]twitter.com/","api","/1.1/"));
}
