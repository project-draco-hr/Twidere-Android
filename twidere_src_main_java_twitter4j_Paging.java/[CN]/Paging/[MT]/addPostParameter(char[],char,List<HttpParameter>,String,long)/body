{
  boolean supported=false;
  for (  final char supportedParam : supportedParams) {
    if (supportedParam == paramKey) {
      supported=true;
      break;
    }
  }
  if (!supported && -1 != paramValue)   throw new IllegalStateException("Paging parameter [" + paramName + "] is not supported with this operation.");
  if (-1 != paramValue) {
    pagingParams.add(new HttpParameter(paramName,String.valueOf(paramValue)));
  }
}
