{
  while (reader.hasKeyValue()) {
switch (reader.nextKey()) {
case "id":
{
        id=reader.nextLong();
        break;
      }
case "name":
{
      name=reader.nextString();
      break;
    }
case "screen_name":
{
    screen_name=reader.nextString();
    break;
  }
default :
{
  reader.skipValue();
  break;
}
}
}
}
