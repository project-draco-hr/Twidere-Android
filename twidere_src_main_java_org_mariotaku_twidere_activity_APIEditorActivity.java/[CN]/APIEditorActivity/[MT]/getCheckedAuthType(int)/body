{
switch (checkedId) {
case R.id.xauth:
{
      return ParcelableCredentials.AUTH_TYPE_XAUTH;
    }
case R.id.basic:
{
    return ParcelableCredentials.AUTH_TYPE_BASIC;
  }
case R.id.twip_o:
{
  return ParcelableCredentials.AUTH_TYPE_TWIP_O_MODE;
}
default :
{
return ParcelableCredentials.AUTH_TYPE_OAUTH;
}
}
}
