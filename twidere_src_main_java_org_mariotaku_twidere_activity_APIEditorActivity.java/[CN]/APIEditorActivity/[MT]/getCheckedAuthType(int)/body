{
switch (checkedId) {
case R.id.xauth:
{
      return ParcelableCredentials.AuthType.XAUTH;
    }
case R.id.basic:
{
    return ParcelableCredentials.AuthType.BASIC;
  }
case R.id.twip_o:
{
  return ParcelableCredentials.AuthType.TWIP_O_MODE;
}
default :
{
return ParcelableCredentials.AuthType.OAUTH;
}
}
}
