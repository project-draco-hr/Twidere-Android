{
  final int uri_code=COMMAND_URI_MATCHER.match(uri);
  checkInsertPermission(uri_code);
  try {
switch (uri_code) {
case CODE_REFRESH_ALL:
{
        mTwitterWrapper.refreshAll();
        break;
      }
default :
    return false;
}
}
 catch (final RuntimeException e) {
e.printStackTrace();
if (Thread.currentThread().getId() != 1) throw new IllegalStateException("This method cannot be called from non-UI thread");
}
return true;
}
