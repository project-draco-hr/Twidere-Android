{
  if (array == null || values == null)   return false;
  for (  final Object item : array) {
    for (    final Object value : values) {
      if (item == null || value == null) {
        if (item == value)         return true;
        continue;
      }
      if (item.equals(value))       return true;
    }
  }
  return false;
}
