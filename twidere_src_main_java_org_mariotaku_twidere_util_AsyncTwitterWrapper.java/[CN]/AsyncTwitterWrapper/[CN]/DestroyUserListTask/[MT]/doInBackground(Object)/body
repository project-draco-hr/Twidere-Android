{
  final Twitter twitter=TwitterAPIUtils.getTwitterInstance(mContext,mAccountId,false);
  if (twitter != null) {
    try {
      if (mListId > 0) {
        final ParcelableUserList list=new ParcelableUserList(twitter.destroyUserList(mListId),mAccountId);
        return SingleResponse.getInstance(list);
      }
    }
 catch (    final TwitterException e) {
      return SingleResponse.getInstance(e);
    }
  }
  return SingleResponse.getInstance();
}
