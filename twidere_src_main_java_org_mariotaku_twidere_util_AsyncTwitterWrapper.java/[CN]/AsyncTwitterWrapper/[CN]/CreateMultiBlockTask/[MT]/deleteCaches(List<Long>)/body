{
  for (  final Uri uri : TwidereDataStore.STATUSES_URIS) {
  }
  final ContentValues values=new ContentValues();
  values.put(CachedRelationships.BLOCKING,true);
  values.put(CachedRelationships.FOLLOWING,false);
  values.put(CachedRelationships.FOLLOWED_BY,false);
  mResolver.update(CachedRelationships.CONTENT_URI,values,Expression.inArgs(CachedRelationships.USER_ID,list.size()).getSQL(),TwidereCollectionUtils.toStringArray(list));
}
