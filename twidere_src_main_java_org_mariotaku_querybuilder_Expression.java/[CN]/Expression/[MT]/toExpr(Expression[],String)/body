{
  final StringBuilder builder=new StringBuilder();
  builder.append('(');
  final int length=array.length;
  for (int i=0; i < length; i++) {
    if (i > 0) {
      builder.append(String.format(" %s ",token));
    }
    builder.append(array[i].getSQL());
  }
  builder.append(')');
  return builder.toString();
}
