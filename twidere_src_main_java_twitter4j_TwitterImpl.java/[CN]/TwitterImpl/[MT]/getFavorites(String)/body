{
  ensureAuthorizationEnabled();
  final String url=conf.getRestBaseURL() + ENDPOINT_FAVORITES_LIST;
  final String signUrl=conf.getSigningRestBaseURL() + ENDPOINT_FAVORITES_LIST;
  return factory.createStatusList(get(url,signUrl,new HttpParameter("screen_name",screenName),INCLUDE_ENTITIES,INCLUDE_REPLY_COUNT,INCLUDE_DESCENDENT_REPLY_COUNT));
}
