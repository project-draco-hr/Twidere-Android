{
switch (item.getItemId()) {
case MENU_HOME:
{
      onBackPressed();
      break;
    }
case MENU_CENTER:
{
    final Fragment fragment=getSupportFragmentManager().findFragmentById(R.id.main_content);
    if ((fragment instanceof IMapFragment)) {
      ((IMapFragment)fragment).center();
    }
    break;
  }
}
return true;
}
