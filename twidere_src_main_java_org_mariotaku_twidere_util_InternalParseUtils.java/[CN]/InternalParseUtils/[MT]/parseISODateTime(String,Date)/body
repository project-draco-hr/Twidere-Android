{
  try {
    return DateFormatUtils.ISO_DATETIME_TIME_ZONE_FORMAT.parse(str);
  }
 catch (  ParseException e) {
    return def;
  }
catch (  Error nsme) {
    try {
      return DateUtils.parseDate(str,Locale.ENGLISH,"yyyy-MM-dd'T'HH:mm:ssZZ");
    }
 catch (    ParseException e1) {
      return def;
    }
  }
}
