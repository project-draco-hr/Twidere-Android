{
  final FrameLayout mainContent=(FrameLayout)findViewById(R.id.main_content);
  if (mainContent != null) {
    return mainContent;
  }
  @SuppressLint("InflateParams") final View view=getLayoutInflater().inflate(R.layout.activity_settings,null);
  return (FrameLayout)view.findViewById(R.id.main_content);
}
