{
  final long listId=args.getLong(EXTRA_LIST_ID,-1);
  final UserKey accountKey=args.getParcelable(EXTRA_ACCOUNT_KEY);
  final UserKey userKey=args.getParcelable(EXTRA_USER_KEY);
  final String screenName=args.getString(EXTRA_SCREEN_NAME);
  final String listName=args.getString(EXTRA_LIST_NAME);
  final UserListSubscribersLoader loader=new UserListSubscribersLoader(context,accountKey,listId,userKey,screenName,listName,getData(),fromUser);
  loader.setCursor(getNextCursor());
  loader.setPage(getNextPage());
  return loader;
}
