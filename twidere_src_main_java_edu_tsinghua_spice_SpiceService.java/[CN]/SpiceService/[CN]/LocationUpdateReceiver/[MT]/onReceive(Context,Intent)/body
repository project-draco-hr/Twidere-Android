{
  if (mLocationManager == null)   return;
  SpiceProfilingUtil.log(context,"AlarmReceiver");
  final String provider=LocationManager.NETWORK_PROVIDER;
  if (mLocationManager.isProviderEnabled(provider)) {
    final Location location=mLocationManager.getLastKnownLocation(provider);
    if (location != null) {
      SpiceProfilingUtil.profile(SpiceService.this,SpiceProfilingUtil.FILE_NAME_LOCATION,location.getTime() + "," + location.getLatitude()+ ","+ location.getLongitude()+ ","+ location.getProvider());
      SpiceProfilingUtil.log(context,location.getTime() + "," + location.getLatitude()+ ","+ location.getLongitude()+ ","+ location.getProvider());
    }
  }
}
