{
  final UserKey accountId=getAccountKey();
  if (accountId != null && mUserId != null && TextUtils.equals(accountId.getId(),mUserId.getId()))   return false;
  final UserKey retweetUserId=status.is_retweet ? status.user_key : null;
  return InternalTwitterContentUtils.isFiltered(database,retweetUserId,status.text_plain,status.spans,status.source,null,status.quoted_user_key);
}
