{
  final ParcelableStatus status=mStatusAdapter.getStatus();
  if (status == null)   return;
  IntentUtils.openMediaDirectly(getActivity(),accountKey,status,media,null,true);
  MediaEvent event=MediaEvent.create(getActivity(),status,media,TimelineType.OTHER,mStatusAdapter.isMediaPreviewEnabled());
  HotMobiLogger.getInstance(getActivity()).log(status.account_key,event);
}
