{
  final ParcelableStatus status=mStatusAdapter.getStatus();
  if (status == null)   return;
  Bundle options=null;
  Utils.openMediaDirectly(getActivity(),accountId,status,media,status.media,options);
  SpiceProfilingUtil.log(getActivity(),status.id + ",Clicked," + status.account_id+ ","+ status.user_id+ ","+ status.text_plain.length()+ ","+ media.preview_url+ ","+ media.media_url+ ","+ TypeMappingUtil.getMediaType(media.type)+ ","+ mStatusAdapter.isMediaPreviewEnabled()+ ","+ status.timestamp);
  SpiceProfilingUtil.profile(getActivity(),status.account_id,status.id + ",Clicked," + status.account_id+ ","+ status.user_id+ ","+ status.text_plain.length()+ ","+ media.preview_url+ ","+ media.media_url+ ","+ TypeMappingUtil.getMediaType(media.type)+ ","+ mStatusAdapter.isMediaPreviewEnabled()+ ","+ status.timestamp);
}
