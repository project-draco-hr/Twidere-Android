{
switch (viewType) {
case VIEW_TYPE_STATUS:
{
      return ListParcelableStatusesAdapter.createStatusViewHolder(mDummyAdapter,mInflater,parent,mCompact,mCardBackgroundColor);
    }
case VIEW_TYPE_USER:
{
    return ParcelableUsersAdapter.createUserViewHolder(mDummyAdapter,mInflater,parent,mCardBackgroundColor);
  }
case VIEW_TYPE_USER_LIST:
{
  return ParcelableUserListsAdapter.createUserListViewHolder(mDummyAdapter,mInflater,parent,mCardBackgroundColor);
}
}
throw new UnsupportedOperationException();
}
