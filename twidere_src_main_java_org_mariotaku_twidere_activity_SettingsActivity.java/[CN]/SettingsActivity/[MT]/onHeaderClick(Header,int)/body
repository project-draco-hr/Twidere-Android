{
  if (header.id == HEADER_ID_RESTORE_ICON) {
    final ComponentName main=new ComponentName(this,MainActivity.class);
    final ComponentName main2=new ComponentName(this,MainHondaJOJOActivity.class);
    mPackageManager.setComponentEnabledSetting(main,PackageManager.COMPONENT_ENABLED_STATE_ENABLED,PackageManager.DONT_KILL_APP);
    mPackageManager.setComponentEnabledSetting(main2,PackageManager.COMPONENT_ENABLED_STATE_DISABLED,PackageManager.DONT_KILL_APP);
    Toast.makeText(this,R.string.icon_restored_message,Toast.LENGTH_SHORT).show();
    finish();
    return;
  }
  super.onHeaderClick(header,position);
}
