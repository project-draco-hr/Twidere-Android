  mEntriesAdapter.addHeader(getString(R.string.function));
  mEntriesAdapter.addPreference("tabs",R.drawable.ic_action_tab,getString(R.string.tabs),CustomTabsFragment.class);
  mEntriesAdapter.addPreference("extension",R.drawable.ic_action_extension,getString(R.string.extensions),ExtensionsListFragment.class);
  mEntriesAdapter.addPreference("refresh",R.drawable.ic_action_refresh,getString(R.string.refresh),R.xml.preferences_refresh);
  mEntriesAdapter.addPreference("notifications",R.drawable.ic_action_notification,getString(R.string.notifications),R.xml.preferences_notifications);
  mEntriesAdapter.addPreference("network",R.drawable.ic_action_web,getString(R.string.network),R.xml.preferences_network);
  mEntriesAdapter.addPreference("compose",R.drawable.ic_action_status_compose,getString(R.string.compose),R.xml.preferences_compose);
  mEntriesAdapter.addPreference("content",R.drawable.ic_action_twidere_square,getString(R.string.content),R.xml.preferences_content);
  mEntriesAdapter.addPreference("storage",R.drawable.ic_action_storage,getString(R.string.storage),R.xml.preferences_storage);
