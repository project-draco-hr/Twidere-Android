{
  if (Build.VERSION.SDK_INT >= Build.VERSION_CODES.M) {
    final String[] permissions={Manifest.permission.ACCESS_FINE_LOCATION,Manifest.permission.ACCESS_COARSE_LOCATION,Manifest.permission.READ_EXTERNAL_STORAGE,Manifest.permission.WRITE_EXTERNAL_STORAGE};
    requestPermissions(permissions,REQUEST_REQUEST_PERMISSIONS);
  }
 else {
    Utils.getCachedLocation(getActivity());
    super.gotoNextPage();
  }
}
