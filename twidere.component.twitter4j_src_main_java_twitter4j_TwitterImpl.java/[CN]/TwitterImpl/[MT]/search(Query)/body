{
  final String url=conf.getRestBaseURL() + ENDPOINT_SEARCH_TWEETS;
  final String signUrl=conf.getSigningRestBaseURL() + ENDPOINT_SEARCH_TWEETS;
  return factory.createQueryResult(get(url,signUrl,query.asHttpParameterArray(INCLUDE_ENTITIES,INCLUDE_RTS,INCLUDE_REPLY_COUNT,INCLUDE_DESCENDENT_REPLY_COUNT)),query);
}
